{
  "botium": {
    "Capabilities": {
      "PROJECTNAME": "telkomsel-chatbot-test",
      "CONTAINERMODE": "webdriverio",
      "SAFEDIR": ".",
      "CLEANUPTEMPDIR": false,
      "WEBDRIVERIO_URL": "https://www.telkomsel.com/support/contact-us",
      "WEBDRIVERIO_OPENBOT": "./actions/open_bot.js",
      "WEBDRIVERIO_BROWSER": "chrome",
      "WEBDRIVERIO_WAITFOR_TIMEOUT": 10000,
      "WEBDRIVERIO_WAITFOR_INTERVAL": 1000,
      "WEBDRIVERIO_OPTIONS": {
        "hostname": "localhost",
        "port": 4444,
        "path": "/wd/hub",
        "capabilities": {
          "browserName": "chrome",
          "goog:chromeOptions": {
            "args": ["--start-maximized"]
          }
        }
      },
      "WEBDRIVERIO_INPUT_ELEMENT": "textarea[id='va-ui-textInput']",
      "WEBDRIVERIO_SEND_BUTTON": "button[class='va-ui-send-button']"
    },
    "Sources": {
      "webdriverio": {
        "type": "webdriverio"
      }
    },
    "Envs": {}
  }
}